{
  "name": "eleventy-high-performance-blog",
  "version": "5.0.2",
  "license": "MIT",
  "description": "A starter repository for a blog web site using the Eleventy static site generator.",
  "scripts": {
    "build-ci": "(npm run js-build || true) && eleventy && npm run test",
    "build": "npm run clean && npm run build-ci",
    "build:prod": "npm run clean && CI=true eleventy",
    "serve:prod": "./serve-prod.sh",
    "watch": "concurrently \"npm run serve\" \"npm run js-build-watch\" \"npm run test-watch\"",
    "serve": "npm run clean && eleventy --serve",
    "js-build": "rollup -c rollup.config.js",
    "js-build-watch": "rollup -c rollup.config.js -w",
    "debug": "DEBUG=* eleventy && npm run test",
    "test": "mocha test/test*.js",
    "test-watch": "mocha test/test*.js --watch --watch-files=test/*",
    "clean": "rm -Rf ./docs/posts/*"
  },
  "pre-push": [
    "build"
  ],
  "pre-commit": [],
  "repository": {
    "type": "git",
    "url": "git://github.com/11ty/eleventy-base-blog.git"
  },
  "bugs": {
    "url": "https://github.com/11ty/eleventy-base-blog/issues"
  },
  "homepage": "https://github.com/11ty/eleventy-base-blog#readme",
  "overrides": {
    "jsdom": "^24.0.0",
    "cross-spawn": "^7.0.3",
    "postcss": "^8.4.31",
    "terser": "^5.14.2"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-navigation": "^0.1.3",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "eleventy-plugin-local-images": "^0.4.0",
    "eleventy-plugin-mathjax": "^2.0.4",
    "file-type": "^16.5.4",
    "fs-extra": "^8.1.0",
    "jsdom": "^24.0.0",
    "luxon": "^2.5.2",
    "markdown-it": "^12.3.2",
    "markdown-it-anchor": "^5.2.5",
    "node-fetch": "^2.6.7",
    "pre-commit": "^1.0.10",
    "pre-push": "^0.1.1",
    "shorthash": "0.0.2"
  },
  "dependencies": {
    "@ampproject/toolbox-optimizer": "^2.4.0",
    "@fontsource/literata": "^5.1.0",
    "@rollup/plugin-terser": "^0.4.4",
    "any-shell-escape": "^0.1.1",
    "browser-sync": "^3.0.3",
    "clean-css": "^4.2.3",
    "concurrently": "^7.0.0",
    "csp-hash-generator": "0.0.3",
    "csso": "^4.0.3",
    "datauri": "^3.0.0",
    "expect.js": "^0.3.1",
    "ffmpeg-static": "^4.4.0",
    "hasha": "^5.2.0",
    "html-minifier-terser": "^7.2.0",
    "image-size": "^0.8.3",
    "jimp": "^0.22.10",
    "lru-cache": "^5.1.1",
    "mocha": "^10.1.0",
    "phin": "^3.7.1",
    "purge-from-html": "^1.0.3",
    "purgecss": "^4.0.3",
    "querystring": "^0.2.0",
    "rollup": "^2.23.0",
    "sharp": "^0.32.6",
    "touch": "^3.1.0"
  }
}
